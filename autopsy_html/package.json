{
  "name": "autopsy_html",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:live": "vite --mode live",
    "build": "vite build && mv dist/index.html ../autopsy/template.html",
    "build:live": "vite build --mode live && mv dist/index.html ../autopsy/live.html",
    "build:vscode": "vite build --mode vscode && cp dist/index.html ../autopsy_vscode/media/webview.html",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "dependencies": {
    "jmespath": "^0.16.0",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/jmespath": "^0.15.2",
    "@types/node": "^24.10.0",
    "svelte": "^5.43.5",
    "svelte-check": "^4.3.3",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-singlefile": "^2.3.0"
  }
}
