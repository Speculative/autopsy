{
  "name": "autopsy-viewer",
  "displayName": "Autopsy Viewer",
  "description": "Live debugging visualization for Python autopsy logs",
  "version": "0.0.1",
  "publisher": "autopsy",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Debuggers",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "commands": [
      {
        "command": "autopsy.openViewer",
        "title": "Autopsy: Open Viewer"
      },
      {
        "command": "autopsy.showLogs",
        "title": "Autopsy: Show Logs"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
